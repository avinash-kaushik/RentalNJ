@page
@model coreproj.Pages.ApplicantPage.CreateModel
@{
    ViewData["Title"] = "Create";
    
}

<h1>Add a new Applicant</h1>

<form method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <label asp-for="@Model.obj.FirstName"></label>
    <input asp-for="@Model.obj.FirstName" />
    </br/></br>
    <label asp-for="@Model.obj.LastName"></label>
    <input asp-for="@Model.obj.LastName" />
    </br/></br>
    <label asp-for="@Model.obj.EmailAddress"></label>
    <input asp-for="@Model.obj.EmailAddress" />
    </br/></br>
    <label asp-for="@Model.obj.Password"></label>
    <input asp-for="@Model.obj.Password" />
    <br />
    <label asp-for="@Model.obj.ConfirmPassword"></label>
    <input asp-for="@Model.obj.ConfirmPassword" />

    <button type="submit"> Save</button>

 </form>